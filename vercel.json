{
    "extends": "@vercel/node/tsconfig.json",
    "compilerOptions": {
        "target": "ES2020",
        "module": "commonjs",
        "strict": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["server.js", "js/**/*"],
    "exclude": ["node_modules"]
}
